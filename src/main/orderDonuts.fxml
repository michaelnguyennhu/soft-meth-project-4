<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.*?>
<Pane xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
      prefHeight="471.0" prefWidth="583.0" xmlns="http://javafx.com/javafx/15.0.1"
      fx:controller="main.OrderDonutsController">
    <children>
        <TextField fx:id="subtotalTextField" editable="false" layoutX="314.0" layoutY="280.0" prefHeight="46.0"
                   prefWidth="240.0" text="\$0.00">
            <font>
                <Font name="Georgia" size="18.0"/>
            </font>
        </TextField>
        <Text layoutX="180.0" layoutY="81.0" strokeType="OUTSIDE" strokeWidth="0.0" text="DONUTS">
            <font>
                <Font name="Georgia Bold" size="48.0"/>
            </font>
        </Text>
        <Button layoutX="76.0" layoutY="377.0" mnemonicParsing="false" onAction="#backToMainMenu" prefHeight="46.0"
                prefWidth="148.0" text="Back to Main Menu">
            <font>
                <Font name="Georgia" size="14.0"/>
            </font>
        </Button>
        <Button layoutX="350.0" layoutY="377.0" mnemonicParsing="false" onAction="#addToOrder" prefHeight="46.0"
                prefWidth="166.0" text="Add Donuts to Order">
            <font>
                <Font name="Georgia" size="14.0"/>
            </font>
        </Button>
        <ComboBox fx:id="donutTypeComboBox" layoutX="53.0" layoutY="166.0" onAction="#donutTypeChosen" prefHeight="40.0"
                  prefWidth="193.0"/>
        <Text layoutX="53.0" layoutY="154.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Type:">
            <font>
                <Font name="Georgia" size="24.0"/>
            </font>
        </Text>
        <ComboBox fx:id="flavorComboBox" layoutX="53.0" layoutY="283.0" onAction="#flavorChosen" prefHeight="40.0"
                  prefWidth="193.0"/>
        <Text layoutX="53.0" layoutY="271.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Flavor:">
            <font>
                <Font name="Georgia" size="24.0"/>
            </font>
        </Text>
        <Text layoutX="385.0" layoutY="271.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Subtotal:">
            <font>
                <Font name="Georgia" size="24.0"/>
            </font>
        </Text>
        <TextField fx:id="quantityTextField" editable="false" layoutX="354.0" layoutY="163.0" prefHeight="46.0"
                   prefWidth="159.0" text="0">
            <font>
                <Font name="Georgia" size="18.0"/>
            </font>
        </TextField>
        <Text layoutX="383.0" layoutY="153.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Quantity:">
            <font>
                <Font name="Georgia" size="24.0"/>
            </font>
        </Text>
        <Button layoutX="524.0" layoutY="173.0" mnemonicParsing="false" onAction="#addQuantity" prefHeight="27.0"
                prefWidth="24.0" text="+">
            <font>
                <Font name="Georgia" size="14.0"/>
            </font>
        </Button>
        <Button layoutX="314.0" layoutY="173.0" mnemonicParsing="false" onAction="#minusQuantity" prefHeight="27.0"
                prefWidth="24.0" text="-">
            <font>
                <Font name="Georgia" size="14.0"/>
            </font>
        </Button>
    </children>
</Pane>
